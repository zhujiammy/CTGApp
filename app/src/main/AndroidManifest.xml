<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.c.ctgapp">

    <uses-permission android:name="android.permission.RECEIVE_USER_PRESENT"/>
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.WRITE_CONTACTS" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <!-- 访问网络，网络定位需要上网 -->
    <uses-permission android:name="android.permission.INTERNET" />
    <!-- 这个权限用于拨打电话 -->
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <!-- 这个权限用于进行网络定位 -->
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <!-- 这个权限用于访问GPS定位 -->
    <uses-permission android:name="android.permission.REQUEST_INSTALL_PACKAGES" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <!-- 写入扩展存储，向扩展卡写入数据，用于写入离线定位数据 -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE">
    </uses-permission>
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:name=".CTGApp"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        tools:ignore="AllowBackup,GoogleAppIndexingWarning,UnusedAttribute"
        android:theme="@style/AppTheme">
        <activity
            android:name=".mvvm.view.LoginActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            tools:ignore="LockedOrientationActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" android:screenOrientation="portrait"
            tools:ignore="InnerclassSeparator,LockedOrientationActivity" />
        <activity android:name=".mvvm.view.my.SwitchingEnterprises.SwitchingEnterprisesActivity" android:screenOrientation="portrait"
            tools:ignore="InnerclassSeparator,LockedOrientationActivity" />
        <activity android:name=".mvvm.view.my.SwitchingEnterprises.CreateEnterpriseActivity" android:screenOrientation="portrait"
            tools:ignore="InnerclassSeparator,LockedOrientationActivity" />
        <activity android:name=".mvvm.view.my.SwitchingEnterprises.ApplicationAccessionActivity" android:screenOrientation="portrait"
            tools:ignore="InnerclassSeparator,LockedOrientationActivity" />
        <activity android:name=".mvvm.view.my.PersonalInformationActivity" android:screenOrientation="portrait"
            tools:ignore="InnerclassSeparator,LockedOrientationActivity" />
        <activity android:name=".mvvm.view.my.PersonalAuthenticationActivity" android:screenOrientation="portrait"
            tools:ignore="InnerclassSeparator,LockedOrientationActivity" />
        <activity android:name=".mvvm.view.my.BusinessFriend.BusinessFriendActivity" android:screenOrientation="portrait"
            tools:ignore="InnerclassSeparator,LockedOrientationActivity" />
        <activity android:name=".mvvm.view.my.BusinessFriend.GroupingActivity" android:screenOrientation="portrait"
            tools:ignore="InnerclassSeparator,LockedOrientationActivity" />
        <activity android:name=".mvvm.view.my.SetActivity" android:screenOrientation="portrait"
            tools:ignore="InnerclassSeparator,LockedOrientationActivity" />
        <activity android:name=".mvvm.view.my.ChangePasswordActivity" android:screenOrientation="portrait"
            tools:ignore="InnerclassSeparator,LockedOrientationActivity" />
        <activity android:name=".mvvm.view.my.ReceivingAddressActivity" android:screenOrientation="portrait"
            tools:ignore="InnerclassSeparator,LockedOrientationActivity" />
        <activity android:name=".mvvm.view.my.AddAddressActivity" android:screenOrientation="portrait"
            tools:ignore="InnerclassSeparator,LockedOrientationActivity" />
        <activity android:name=".mvvm.view.my.AttentionNotesActivity" android:screenOrientation="portrait"
            tools:ignore="InnerclassSeparator,LockedOrientationActivity" />
        <activity android:name=".mvvm.view.my.QueryrecordActivity" android:screenOrientation="portrait"
            tools:ignore="InnerclassSeparator,LockedOrientationActivity" />
        <activity android:name=".mvvm.view.my.MyBusinessActivity" android:screenOrientation="portrait"
            tools:ignore="InnerclassSeparator,LockedOrientationActivity" />
        <activity android:name=".mvvm.view.my.MyTeamActivity" android:screenOrientation="portrait"
            tools:ignore="InnerclassSeparator,LockedOrientationActivity" />
        <activity android:name=".mvvm.view.my.AddStaffActivity" android:screenOrientation="portrait"
            tools:ignore="InnerclassSeparator,LockedOrientationActivity" />
        <activity android:name=".mvvm.view.my.BusinessFriend.AddFriendsActivity" android:screenOrientation="portrait"
            tools:ignore="InnerclassSeparator,LockedOrientationActivity" />
        <activity android:name=".mvvm.view.my.BusinessFriend.MaillistActivity" android:screenOrientation="portrait"
            tools:ignore="InnerclassSeparator,LockedOrientationActivity" />
        <activity android:name=".mvvm.view.my.BusinessFriend.MaillistActivitys" android:screenOrientation="portrait"
            tools:ignore="InnerclassSeparator,LockedOrientationActivity" />
        <activity android:name=".mvvm.view.my.BusinessFriend.MyBusinessFriendActvity" android:screenOrientation="portrait"
            tools:ignore="InnerclassSeparator,LockedOrientationActivity" />
        <activity android:name=".mvvm.view.my.BusinessFriend.ApplicationNoticeActivity" android:screenOrientation="portrait"
            tools:ignore="InnerclassSeparator,LockedOrientationActivity" />
        <activity android:name=".mvvm.view.my.CustomerManagementActivity" android:screenOrientation="portrait"
            tools:ignore="InnerclassSeparator,LockedOrientationActivity" />
        <activity android:name=".mvvm.view.AccountRegistrationActivity" android:screenOrientation="portrait"
            tools:ignore="InnerclassSeparator,LockedOrientationActivity" />
        <activity android:name=".mvvm.view.ForgetPasswordActivity" android:screenOrientation="portrait"
            tools:ignore="InnerclassSeparator,LockedOrientationActivity" />

        <!-- Since JCore2.0.0 Required SDK核心功能-->
        <!-- 这个Service要继承JCommonService -->
        <service android:name="com.c.ctgapp.Service.SeviceCTG"
            android:process=":pushcore">
            <intent-filter>
                <action android:name="cn.jiguang.user.service.action" />
            </intent-filter>
        </service>
        <provider
            android:name=".Tools.FileProvider"
            android:authorities="${applicationId}.fileProvider"
            android:exported="false"
            android:grantUriPermissions="true"
            tools:replace="name,authorities,exported,grantUriPermissions">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/file_paths"
                tools:replace="name,resource"/>
        </provider>
    </application>

</manifest>