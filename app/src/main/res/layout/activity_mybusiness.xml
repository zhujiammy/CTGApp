<androidx.coordinatorlayout.widget.CoordinatorLayout android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:fitsSystemWindows="true"
    android:background="@color/white"
    tools:context=".mvvm.view.my.MyBusinessActivity"
    tools:ignore="MergeRootFrame"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <com.google.android.material.appbar.AppBarLayout
        android:id="@+id/appbar"
        android:layout_width="match_parent"
        android:layout_height="257dp"
        android:fitsSystemWindows="true"
        android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
        android:stateListAnimator="@animator/show_toolbar"
        android:animateLayoutChanges="true">

        <com.google.android.material.appbar.CollapsingToolbarLayout
            app:contentScrim="@color/white"
            android:id="@+id/toolbar_layout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fitsSystemWindows="true"
            app:collapsedTitleGravity="center"
            app:collapsedTitleTextAppearance="@style/TextAppearance.Sunflower.Toolbar.Text"
            app:layout_scrollFlags="scroll|exitUntilCollapsed"
            app:titleEnabled="false"
            app:toolbarId="@id/toolbar">

            <LinearLayout
                android:gravity="center"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <ImageView
                    android:layout_gravity="center"
                    android:fitsSystemWindows="true"
                    android:scaleType="centerCrop"
                    android:layout_width="58dp"
                    android:layout_height="58dp"
                    app:layout_collapseMode="parallax"
                    android:src="@mipmap/takephoto"
                    />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="?attr/actionBarSize"
                    android:text="无锡泽润物流"
                    android:textColor="@color/white"
                    android:textSize="20sp"
                    android:gravity="center"
                    />
                <TextView
                    android:layout_below="@id/top_tv"
                    android:textSize="13sp"
                    android:layout_width="50dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/tv1_selector"
                    android:gravity="center"
                    android:paddingTop="2dp"
                    android:paddingBottom="2dp"
                    android:text="制造商"
                    android:textColor="@color/tv11"
                    />
            </LinearLayout>


            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbar"
                android:layout_width="match_parent"
                android:layout_height="?attr/actionBarSize"
                android:background="@android:color/transparent"
                app:layout_collapseMode="pin"
                app:contentInsetStartWithNavigation="0dp"
                app:navigationIcon="@drawable/ic_detail_back"
                app:menu="@menu/menu_plant_detail" />

        </com.google.android.material.appbar.CollapsingToolbarLayout>

    </com.google.android.material.appbar.AppBarLayout>

    <androidx.core.widget.NestedScrollView
        android:background="@color/lin"
        android:id="@+id/plant_detail_scrollview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:clipToPadding="false"
        android:paddingBottom="@dimen/fab_bottom_padding"
        app:layout_behavior="@string/appbar_scrolling_view_behavior">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <androidx.constraintlayout.widget.ConstraintLayout
                android:orientation="vertical"
                android:elevation="3dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/lin_selector"
                android:layout_marginRight="10dp"
                android:layout_marginLeft="10dp"
                android:layout_width="match_parent"
                android:layout_height="200dp">


                <TextView
                    android:id="@+id/textView14"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="18dp"
                    android:layout_marginTop="19dp"
                    android:text="公司电话"
                    android:textColor="@color/tv5"
                    android:textSize="11sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.103"
                    tools:ignore="MissingConstraints" />

                <TextView
                    android:id="@+id/textView15"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="0.5dp"
                    android:text="8938-40475615"
                    android:textColor="@color/tv2"
                    android:textSize="14sp"
                    app:layout_constraintStart_toStartOf="@+id/textView14"
                    app:layout_constraintTop_toBottomOf="@+id/textView14"
                    tools:ignore="MissingConstraints"/>

                <ImageView
                    android:id="@+id/edit_iv"
                    android:layout_width="18dp"
                    android:layout_height="18dp"
                    android:layout_marginEnd="4dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.046"
                    app:srcCompat="@mipmap/editp"
                    tools:ignore="MissingConstraints" />

                <TextView

                    android:id="@+id/textView16"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="19dp"
                    android:layout_marginEnd="115dp"
                    android:text="公司邮箱"
                    android:textColor="@color/tv5"
                    android:textSize="11sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.102"
                    tools:ignore="MissingConstraints" />

                <TextView
                    android:id="@+id/textView17"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="fengtian@ctgoe.com"
                    android:textColor="@color/tv2"
                    android:textSize="14sp"
                    android:layout_marginTop="0.5dp"
                    app:layout_constraintStart_toStartOf="@+id/textView16"
                    app:layout_constraintTop_toBottomOf="@+id/textView16"
                    tools:ignore="MissingConstraints" />

                <TextView
                    android:id="@+id/textView18"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="公司网址"
                    android:textColor="@color/tv5"
                    android:textSize="11sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="@+id/textView15"
                    app:layout_constraintTop_toBottomOf="@+id/textView15"
                    app:layout_constraintVertical_bias="0.192"
                    tools:ignore="MissingConstraints" />

                <TextView
                    android:id="@+id/textView19"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="www.fcewcfdw.com"
                    android:textColor="@color/tv2"
                    android:textSize="14sp"
                    android:layout_marginTop="0.5dp"
                    app:layout_constraintStart_toStartOf="@+id/textView18"
                    app:layout_constraintTop_toBottomOf="@+id/textView18"
                    tools:ignore="MissingConstraints" />

                <TextView
                    android:id="@+id/textView20"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="公司规模"
                    android:textColor="@color/tv5"
                    android:textSize="11sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="@+id/textView17"
                    app:layout_constraintTop_toBottomOf="@+id/textView17"
                    app:layout_constraintVertical_bias="0.2"
                    tools:ignore="MissingConstraints" />

                <TextView
                    android:layout_marginTop="0.5dp"
                    android:id="@+id/textView21"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="50"
                    android:textColor="@color/tv2"
                    android:textSize="14sp"
                    app:layout_constraintStart_toStartOf="@+id/textView20"
                    app:layout_constraintTop_toBottomOf="@+id/textView20"
                    tools:ignore="MissingConstraints" />

                <TextView
                    android:id="@+id/textView22"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="公司地址"
                    android:textColor="@color/tv5"
                    android:textSize="11sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="@+id/textView19"
                    app:layout_constraintTop_toBottomOf="@+id/textView19"
                    app:layout_constraintVertical_bias="0.362"
                    tools:ignore="MissingConstraints" />

                <TextView
                    android:layout_marginTop="0.5dp"
                    android:id="@+id/textView23"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="江苏省常州市新北区江海路599号"
                    android:textColor="@color/tv2"
                    android:textSize="14sp"
                    app:layout_constraintStart_toStartOf="@+id/textView22"
                    app:layout_constraintTop_toBottomOf="@+id/textView22"
                    tools:ignore="MissingConstraints" />
            </androidx.constraintlayout.widget.ConstraintLayout>
            
            <RelativeLayout
                android:layout_marginRight="10dp"
                android:layout_marginLeft="10dp"
                android:layout_width="match_parent"
                android:layout_height="40dp">
                <TextView
                    android:layout_centerVertical="true"
                    android:textSize="11sp"
                    android:textColor="@color/tv5"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:text="经营信息"
                    />

                <TextView
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:textSize="11sp"
                    android:textColor="@color/tv12"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:text="编辑"
                    />
            </RelativeLayout>
            
            <HorizontalScrollView
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_width="match_parent"
                android:layout_height="160dp">
                <LinearLayout
                    android:padding="5dp"
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent">
                    <LinearLayout
                        android:elevation="3dp"
                        android:layout_marginRight="7dp"
                        android:orientation="vertical"
                        android:background="@drawable/lin_selector"
                        android:layout_width="124dp"
                        android:layout_height="match_parent">
                        <TextView
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="10dp"
                            android:textColor="@color/tv5"
                            android:textSize="11sp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="经营范围"
                            />
                        <TextView
                            android:gravity="center"
                            android:background="@drawable/tv_lable_selector"
                            android:layout_marginLeft="15dp"
                            android:layout_marginTop="10dp"
                            android:textColor="@color/tv11"
                            android:textSize="10sp"
                            android:layout_width="44dp"
                            android:layout_height="17dp"
                            android:text="全车件"
                            />


                    </LinearLayout>
                    <LinearLayout
                        android:elevation="3dp"
                        android:layout_marginRight="7dp"
                        android:orientation="vertical"
                        android:background="@drawable/lin_selector"
                        android:layout_width="124dp"
                        android:layout_height="match_parent">
                        <TextView
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="10dp"
                            android:textColor="@color/tv5"
                            android:textSize="11sp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="经营范围"
                            />
                        <TextView
                            android:gravity="center"
                            android:background="@drawable/tv_lable_selector"
                            android:layout_marginLeft="15dp"
                            android:layout_marginTop="10dp"
                            android:textColor="@color/tv11"
                            android:textSize="10sp"
                            android:layout_width="44dp"
                            android:layout_height="17dp"
                            android:text="全车件"
                            />


                    </LinearLayout>
                    <LinearLayout
                        android:elevation="3dp"
                        android:layout_marginRight="7dp"
                        android:orientation="vertical"
                        android:background="@drawable/lin_selector"
                        android:layout_width="124dp"
                        android:layout_height="match_parent">
                        <TextView
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="10dp"
                            android:textColor="@color/tv5"
                            android:textSize="11sp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="经营范围"
                            />
                        <TextView
                            android:gravity="center"
                            android:background="@drawable/tv_lable_selector"
                            android:layout_marginLeft="15dp"
                            android:layout_marginTop="10dp"
                            android:textColor="@color/tv11"
                            android:textSize="10sp"
                            android:layout_width="44dp"
                            android:layout_height="17dp"
                            android:text="全车件"
                            />


                    </LinearLayout>
                    <LinearLayout
                        android:elevation="3dp"
                        android:layout_marginRight="7dp"
                        android:orientation="vertical"
                        android:background="@drawable/lin_selector"
                        android:layout_width="124dp"
                        android:layout_height="match_parent">
                        <TextView
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="10dp"
                            android:textColor="@color/tv5"
                            android:textSize="11sp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="经营范围"
                            />
                        <TextView
                            android:gravity="center"
                            android:background="@drawable/tv_lable_selector"
                            android:layout_marginLeft="15dp"
                            android:layout_marginTop="10dp"
                            android:textColor="@color/tv11"
                            android:textSize="10sp"
                            android:layout_width="44dp"
                            android:layout_height="17dp"
                            android:text="全车件"
                            />


                    </LinearLayout>

                </LinearLayout>

            </HorizontalScrollView>
            <RelativeLayout
                android:layout_marginRight="10dp"
                android:layout_marginLeft="10dp"
                android:layout_width="match_parent"
                android:layout_height="40dp">
                <TextView
                    android:layout_centerVertical="true"
                    android:textSize="11sp"
                    android:textColor="@color/tv5"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:text="更多信息"
                    />

            </RelativeLayout>
            <LinearLayout
                android:layout_marginBottom="10dp"
                android:padding="3dp"
                android:orientation="vertical"
                android:elevation="3dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/lin_selector"
                android:layout_marginRight="10dp"
                android:layout_marginLeft="10dp"
                android:layout_width="match_parent"
                android:layout_height="150dp">

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    
                    <LinearLayout
                        android:id="@+id/Companyprofile_lin"
                        android:clickable="true"
                        android:background="@drawable/linerlayout_selected"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">
                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@mipmap/gsgl"/>
                        <TextView
                            android:layout_marginTop="2dp"
                            android:textSize="12sp"
                            android:textColor="@color/tv5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="公司简介"
                            />

                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/Contacts_lin"
                        android:clickable="true"
                        android:background="@drawable/linerlayout_selected"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">
                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@mipmap/lxr"/>
                        <TextView
                            android:layout_marginTop="2dp"
                            android:textSize="12sp"
                            android:textColor="@color/tv5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="对外联系人"
                            />

                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/Accountinformation_lin"
                        android:clickable="true"
                        android:background="@drawable/linerlayout_selected"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">
                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@mipmap/rzsh"/>
                        <TextView
                            android:layout_marginTop="2dp"
                            android:textSize="12sp"
                            android:textColor="@color/tv5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="账户信息"
                            />

                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/Invoiceinformation_lin"
                        android:clickable="true"
                        android:background="@drawable/linerlayout_selected"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">
                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@mipmap/tx"/>
                        <TextView
                            android:layout_marginTop="2dp"
                            android:textSize="12sp"
                            android:textColor="@color/tv5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="开票信息"
                            />

                    </LinearLayout>

                </LinearLayout>
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <LinearLayout
                        android:id="@+id/Receivinginformation_lin"
                        android:clickable="true"
                        android:background="@drawable/linerlayout_selected"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">
                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@mipmap/hc"/>
                        <TextView
                            android:layout_marginTop="2dp"
                            android:textSize="12sp"
                            android:textColor="@color/tv5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="收货信息"
                            />

                    </LinearLayout>
                    <LinearLayout
                        android:clickable="true"
                        android:background="@drawable/linerlayout_selected"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">
                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@mipmap/zlrz"/>
                        <TextView
                            android:layout_marginTop="2dp"
                            android:textSize="12sp"
                            android:textColor="@color/tv5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="宣传资料"
                            />

                    </LinearLayout>
                    <LinearLayout
                        android:clickable="true"
                        android:background="@drawable/linerlayout_selected"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">
                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@mipmap/ry"/>
                        <TextView
                            android:layout_marginTop="2dp"
                            android:textSize="12sp"
                            android:textColor="@color/tv5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="荣誉资质"
                            />

                    </LinearLayout>
                    <LinearLayout
                        android:clickable="true"
                        android:background="@drawable/linerlayout_selected"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">
                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@mipmap/rz"/>
                        <TextView
                            android:layout_marginTop="2dp"
                            android:textSize="12sp"
                            android:textColor="@color/tv5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="企业认证"
                            />

                    </LinearLayout>

                </LinearLayout>
            </LinearLayout>

        </LinearLayout>


    </androidx.core.widget.NestedScrollView>

</androidx.coordinatorlayout.widget.CoordinatorLayout>